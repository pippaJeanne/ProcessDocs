                        <reg type="modernisation">
                                <xsl:if test="matches(.,'^[Tt]reshaultz$')">
                                <xsl:value-of select="substring-before(.,'reshaultz')"/>rès hauts</xsl:if>
                                <xsl:if test="matches(.,'^TRESHAULTZ$')">TRÈS HAUTS</xsl:if>
                         </reg>
                                </choice>
                </xsl:when>
<xsl:when test="matches(.,'^plustost$', 'i')">
